{
  "expo": {
    "owner": "pokerdealer",
    "name": "Poker Dealer",
    "slug": "PokerManagerProto",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/poker-chip.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/poker-chip.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true,
      "bundleIdentifier": "com.pokerdealer.PokerDealerApp",
      "infoPlist": {
        "UISupportsDocumentBrowser": true,
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/poker-chip.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "Este app acessa sua câmera para que tire fotos de perfil de usuário."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b9e07a3a-213f-4b1e-85ea-9d13bbf4f509"
      }
    }
  }
}
