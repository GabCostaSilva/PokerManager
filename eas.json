{
  "cli": {
    "version": ">= 3.15.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://poker-manager-backend.onrender.com"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://poker-manager-backend.onrender.com"
      }
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://poker-manager-backend.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
